{% extends '::base.html.twig' %}
{% block title %}Utilidad editar factura detalle - Soga App{% endblock %}
{% block content %}   
<div class="container">
    {{ form_start(form, { 'attr': {'class': 'form-horizontal'} }) }}
    <h3>Utilidad editar factura detalle</h3>
    <div class="panel panel-default">
        <div class="panel-heading">Informacion general</div>
       <div class="panel-body">         
            <div class="form-group form-group-sm">
                <label class="col-sm-3 control-label">Ciudad:</label>
                <div class="col-sm-9">
                    {{ form_widget(form.ciudadRel, { 'attr': {'class': 'form-control'} }) }}                        
                </div>                
            </div>                          
        </div>
    </div>               
    <div class="modal-footer">
        <a class="btn btn-default btn-sm" href="{{ path('brs_tur_utilidad_editar_factura_detalle') }}">Cancelar</a>        
        {{ form_widget(form.guardar, { 'attr': {'class': 'btn btn-primary btn-sm'} }) }}        
    </div>
    {{ form_end(form) }}
</div>
{% endblock %}