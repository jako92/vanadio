{% extends '::base2.html.twig' %}
{% block title %}Supervigilancia parafiscales - Soga App{% endblock %}
{% block content %}
        <section class="content-header">
        <h1>
            Programacion turnos
            <small>Programaciones: lista</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="{{ path('brasa_general_inicio') }}"><i class="fa fa-dashboard"></i> Inicio</a></li>
            <li><a href="{{ path('brasa_general_inicio') }}"><i class="fa fa-calendar"></i> Programacion turnos</a></li>
            <li><a href="{{ path('brasa_general_inicio') }}"><i class="fa fa-bars"></i> Movimiento</a></li>
            <li class="active">Lista</li>
        </ol>
    </section>    
    <section class="content"> 
    <script type="text/javascript">
        $(document).ready(function () {
            $('.date').datepicker({dateFormat: 'yy-mm-dd', dayNamesMin: ["Do", "Lu", "Ma", "Mi", "Ju", "Vi", "Sa"], monthNames: ["Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio", "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Deciembre"], monthNames: ["Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio", "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Diciembre"]})
        });
    </script>
    <div class="page-header">
        <h3>Supervigilancia parafiscales</h3>
    </div>
    {{ form_start(form, { 'attr': {'class': 'form-horizontal', 'novalidate': 'novalidate'} }) }}
    {{ form_errors(form) }}
    <div class="panel panel-default">
        <div class="panel-heading">Filtros de busqueda</div>
        <div class="panel-body">

            <div class="form-group form-group-sm">
                <label for="form_fechaDesde" class="col-sm-2 control-label">Desde:</label>
                <div class="col-sm-10">
                    {{ form_widget(form.fechaDesde, {'attr': {'maxlength' : 10} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label for="form_fechaHasta" class="col-sm-2 control-label">Hasta:</label>
                <div class="col-sm-10">
                    {{ form_widget(form.fechaHasta, {'attr': {'maxlength' : 10} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <div class="col-sm-offset-2 col-sm-10">

                    {{ form_widget(form.BtnGenerar, { 'attr': {'class': 'btn btn-default btn-sm', 'style':'float:right'} }) }}                        
                </div>                    
            </div>
        </div>
    </div>
    <table  class="table table-striped table-bordered table-condensed table-hover">
        <tr>
            <th>CÃ“DIGO</th>
            <th>MES</th>
            <th>CARGO</th>
            <th>EMPLEADOS</th>
            <th>NOMINA</th>
            <th>EPS</th>
            <th>PENSION</th>
            <th>ARL</th>
            <th>SENA</th>
            <th>ICBF</th>
            <th>CCF</th>
        </tr>
        {% for arSupervigilanciaParafiscales in arSupervigilanciaParafiscales %}
            <tr>
                <td><input type="hidden" name="LblCodigo[]" value="{{ arSupervigilanciaParafiscales.codigoSupervigilanciaParafiscalesPk }}">{{ arSupervigilanciaParafiscales.codigoSupervigilanciaParafiscalesPk }} </td>                
                <td>{{ arSupervigilanciaParafiscales.mes  }}</td>                
                <td>{{ arSupervigilanciaParafiscales.cargo  }}</td>                
                <td style="text-align: right">{{ arSupervigilanciaParafiscales.empleados | number_format(0, '.', ',') }}</td>
                <td style="text-align: right">{{ arSupervigilanciaParafiscales.vrNomina | number_format(0, '.', ',') }}</td>
                <td style="text-align: right">{{ arSupervigilanciaParafiscales.vrEps | number_format(0, '.', ',') }}</td>
                <td style="text-align: right">{{ arSupervigilanciaParafiscales.vrPension | number_format(0, '.', ',') }}</td>
                <td style="text-align: right">{{ arSupervigilanciaParafiscales.vrArl | number_format(0, '.', ',') }}</td>
                <td style="text-align: right">{{ arSupervigilanciaParafiscales.vrSena | number_format(0, '.', ',') }}</td>
                <td style="text-align: right">{{ arSupervigilanciaParafiscales.vrIcbf | number_format(0, '.', ',') }}</td>
                <td style="text-align: right">{{ arSupervigilanciaParafiscales.vrCcf | number_format(0, '.', ',') }}</td>
            </tr>
        {% endfor %}
    </table>
    <div class="btn-toolbar" style="float: left">
        <div class="btn-group">
            {{ knp_pagination_render(arSupervigilanciaParafiscales) }}
        </div>
        <div class="btn-group">
            <div class="btn btn-default btn-sm">Registros <span class="badge">{{ arSupervigilanciaParafiscales.getTotalItemCount }}</span></div>
        </div>
    </div>
    <div class="btn-toolbar" style="float: right">
        <div class="btn-group">                
            {{ form_widget(form.BtnExcel, { 'attr': {'class': 'btn btn-default btn-sm'} }) }}
        </div>
    </div>

    {{ form_rest(form) }}
    {{ form_end(form) }}
        <br/><br/><br/><br/>
    </section>      
{% endblock %}
