{% extends '::base_sin_menu.html.twig' %}
{% block title %}Programacion de pago inconsistencias - Soga App{% endblock %}
{% block content %}
    <section class="content-header">
        <h1>
            Recurso humano
            <small>Programacion de pago: inconsistencia</small>
        </h1>
    </section>    
    <section class="content">        	
        {{ form_start(form, { 'attr': {'class': 'form-horizontal'} }) }}        
        <table  class="table table-striped table-bordered table-condensed table-hover">            
            <tr>
                <th>CÃ“DIGO</th>
                <th>INCONSISTENCIA</th>
            </tr>
            {% for arProgramacionPagoInconsistencias in arProgramacionPagoInconsistencias %}
                <tr>
                    <td><input type="hidden" name="LblCodigo[]" value="{{ arProgramacionPagoInconsistencias.codigoProgramacionPagoInconsistenciaPk }}">{{ arProgramacionPagoInconsistencias.codigoProgramacionPagoInconsistenciaPk }} </td>
                    <td>{{ arProgramacionPagoInconsistencias.inconsistencia  }}</td>
                </tr>
            {% endfor %}                        
        </table>  
        <div class="modal-footer">        
            {{ form_widget(form.BtnLimpiar, { 'attr': {'class': 'btn btn-sm btn-primary'} }) }}            
        </div>     
        {{ form_end(form) }}
        <br/><br/><br/><br/>
    </section>
{% endblock %}