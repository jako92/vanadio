{% extends '::base2.html.twig' %}
{% block title %}Incapacidad diagnosticos nuevo - Soga App{% endblock %} 
{% block content %}
    {{ form_start(formIncapacidadDiagnostico, { 'attr': {'class': 'form-horizontal'} }) }}
    {{ form_errors(formIncapacidadDiagnostico) }}
    <h3>Nuevo zona</h3>
    <div class="panel panel-default">
        <div class="panel-heading">Informacion general</div>
        <div class="panel-body">
            <div class="form-group form-group-sm">
                <label class="col-sm-2 control-label">Nombre:</label>
                <div class="col-sm-10">
                    {{ form_widget(formIncapacidadDiagnostico.nombre, { 'attr': {'style':'text-transform:uppercase;','onblur':'javascript:this.value=this.value.toUpperCase()', 'class': 'form-control','autofocus':'autofocus'} }) }}
                </div>
            </div>
            <div class="form-group form-group-sm">
                <label class="col-sm-2 control-label">Codigo:</label>
                <div class="col-sm-10">
                    {{ form_widget(formIncapacidadDiagnostico.codigo, { 'attr': {'style':'text-transform:uppercase;','onblur':'javascript:this.value=this.value.toUpperCase()', 'class': 'form-control','autofocus':'autofocus'} }) }}
                </div>
            </div>    
        </div>
    </div>
    <div class="modal-footer">
        <a class="btn btn-default btn-sm" href="{{ path('brs_rhu_base_incapacidaddiagnostico') }}">Cancelar</a>
        {{ form_widget(formIncapacidadDiagnostico.guardar, { 'attr': {'class': 'btn btn-primary btn-sm'} }) }}
    </div>            
    {{ form_end(formIncapacidadDiagnostico) }}
{% endblock %}
