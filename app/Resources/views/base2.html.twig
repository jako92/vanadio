<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <title>{% block title %}Soga App!{% endblock %}</title>
        {% block stylesheets %}            
            <!-- Bootstrap 3.3.6 -->
            <link rel="stylesheet" href="{{ asset('recursos/bootstrap/css/bootstrap.min.css') }}">            
            <!-- Font Awesome -->
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
            <!-- Ionicons -->
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
            <!-- Theme style -->
            <link rel="stylesheet" href="{{ asset('recursos/dist/css/AdminLTE.min.css') }}">
            <!-- AdminLTE Skins. Choose a skin from the css/skins
                 folder instead of downloading all of them to reduce the load. -->
            <link rel="stylesheet" href="{{ asset('recursos/dist/css/skins/_all-skins.min.css') }}">
            <!-- iCheck -->
            <link rel="stylesheet" href="{{ asset('recursos/plugins/iCheck/flat/blue.css') }}">
            <!-- Morris chart -->
            <link rel="stylesheet" href="{{ asset('recursos/plugins/morris/morris.css') }}">
            <!-- jvectormap -->
            <link rel="stylesheet" href="{{ asset('recursos/plugins/jvectormap/jquery-jvectormap-1.2.2.css') }}">
            <!-- Date Picker -->
            <link rel="stylesheet" href="{{ asset('recursos/plugins/datepicker/datepicker3.css') }}">
            <!-- Daterange picker -->
            <link rel="stylesheet" href="{{ asset('recursos/plugins/daterangepicker/daterangepicker.css') }}">
            <!-- bootstrap wysihtml5 - text editor -->
            <link rel="stylesheet" href="{{ asset('recursos/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css') }}">
        {% endblock %}                                    

    </head>
    <body class="hold-transition skin-blue sidebar-mini">
        {% block body %}  
            <div class="wrapper">
                <header class="main-header">
                    
                    <!-- Logo -->
                    <a href="{{ path('brasa_general_inicio') }}" class="logo">
                        <!-- mini logo for sidebar mini 50x50 pixels -->
                        <span class="logo-mini"><b>S</b>V1</span>
                        <!-- logo for regular state and mobile devices -->
                        <span class="logo-lg"><b>Soga</b>V1</span>
                    </a>
                    <!-- Header Navbar: style can be found in header.less -->
                    {% include 'barraNavegacion.html.twig' %}
                </header>
                                            
                <!-- Left side column. contains the logo and sidebar -->
                {% include 'menu.html.twig' %}

                <!-- Content Wrapper. Contains page content -->
                <div class="content-wrapper">
                    <!-- Content Header (Page header) -->
                    <section class="content-header">
                        <h1>
                            Dashboard
                            <small>Control panel</small>
                        </h1>
                        <ol class="breadcrumb">
                            <li><a href="{{ path('brasa_general_inicio') }}"><i class="fa fa-dashboard"></i> Home</a></li>
                            <li class="active">Dashboard</li>
                        </ol>
                    </section>

                    <!-- Main content -->
                    <section class="content">
                        {% block content %}

                        {% endblock %}
                    </section>
                    <!-- /.content -->
                </div>

                <!-- /.content-wrapper -->
                <footer class="main-footer">
                    <div class="pull-right hidden-xs">
                        <b>Version</b> 2.3.11
                    </div>
                    <strong>Copyright &copy; 2014-2016 <a href="http://almsaeedstudio.com">Almsaeed Studio</a>.</strong> All rights
                    reserved.
                </footer>

                <!-- Control Sidebar -->
                {% include 'barraConfiguracion.html.twig' %}
                <!-- /.control-sidebar -->
                <!-- Add the sidebar's background. This div must be placed
                     immediately after the control sidebar -->
                <div class="control-sidebar-bg"></div>
            </div>        

        {% endblock %} 
        {% block javascripts %}
            <!-- jQuery 2.2.3 -->
            <script src="{{ asset('recursos/plugins/jQuery/jquery-2.2.3.min.js') }}"></script>
            <!-- jQuery UI 1.11.4 -->
            <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
            <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
            <script>
                $.widget.bridge('uibutton', $.ui.button);
            </script>
            <!-- Bootstrap 3.3.6 -->
            <script src="{{ asset('recursos/bootstrap/js/bootstrap.min.js') }}"></script>
            <!-- Morris.js charts -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
            <script src="{{ asset('recursos/plugins/morris/morris.min.js') }}"></script>
            <!-- Sparkline -->
            <script src="{{ asset('recursos/plugins/sparkline/jquery.sparkline.min.js') }}"></script>
            <!-- jvectormap -->
            <script src="{{ asset('recursos/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js') }}"></script>
            <script src="{{ asset('recursos/plugins/jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>
            <!-- jQuery Knob Chart -->
            <script src="{{ asset('recursos/plugins/knob/jquery.knob.js') }}"></script>
            <!-- daterangepicker -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>
            <script src="{{ asset('recursos/plugins/daterangepicker/daterangepicker.js') }}"></script>
            <!-- datepicker -->
            <script src="{{ asset('recursos/plugins/datepicker/bootstrap-datepicker.js') }}"></script>
            <!-- Bootstrap WYSIHTML5 -->
            <script src="{{ asset('recursos/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js') }}"></script>
            <!-- Slimscroll -->
            <script src="{{ asset('recursos/plugins/slimScroll/jquery.slimscroll.min.js') }}"></script>
            <!-- FastClick -->
            <script src="{{ asset('recursos/plugins/fastclick/fastclick.js') }}"></script>
            <!-- AdminLTE App -->
            <script src="{{ asset('recursos/dist/js/app.min.js') }}"></script>
            <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
            <script src="{{ asset('recursos/dist/js/pages/dashboard.js') }}"></script>
            <!-- AdminLTE for demo purposes -->
            <script src="{{ asset('recursos/dist/js/demo.js') }}"></script> 
            <!-- Funciones personales -->
            <script src="{{ asset('js/general.js') }}"></script>            
        {% endblock %}              
    </body>
</html>